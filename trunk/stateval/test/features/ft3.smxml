<?xml version="1.0"?>
<!DOCTYPE stateval PUBLIC "stateval" "http://stateval.googlecode.com/svn/trunk/stateval/data/dtd/stateval.dtd">

<stateval xmlns:state="stateval">
  
  <states>
    <state name="root" type="CompoundState"/>
    <state name="a" type="InitialState" parent="root"/>
    <state name="b" type="FinalState" parent="root"/>
  </states>

  <transitions>
    <transition from="a" to="b"/>
  </transitions>
  
</stateval>
